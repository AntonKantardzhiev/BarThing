{% extends 'base.html.twig' %}

{% block body %}
<div class="contact-container">
    <div id="contact">
        <div class="contact-box">
            <div class="c-heading">
                <h1>Get in touch</h1>
                <p>Call or email us!</p>
            </div>
            <div class="c-input">
                <input type="text" placeholder="Full Name">
                <input type="email" placeholder="Email">
                <textarea name="message" placeholder="message us"></textarea>
                <button>Send</button>
            </div>
        </div>
        <div class="map">
            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2499.5619253416385!2d4.402991315296096!3d51.20872334031873!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47c3f6e5ac9969cf%3A0x2fe3b8699a339964!2sJustitiestraat%2C%202018%20Antwerpen!5e0!3m2!1snl!2sbe!4v1623852755101!5m2!1snl!2sbe" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
        </div>
    </div>
</div>

{% endblock %}
